{
  "name": "zephyr-programs-scripts",
  "version": "1.0.0",
  "description": "Zephyr Programs for Soroswap",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "token": "node --loader ts-node/esm src/scripts/jwt.ts",
    "pairs:subscribe": "node --loader ts-node/esm src/scripts/pairs-subscribe.ts",
    "pairs:catchups": "node --loader ts-node/esm src/scripts/pairs-catchups.ts",
    "init:catchups": "node --loader ts-node/esm src/scripts/init-catchups.ts",
    "catchups": "yarn build && node --trace-deprecation dist/pairs-catchups.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stellar/stellar-sdk": "^12.0.1",
    "@types/node": "^20.14.0",
    "dotenv": "^16.4.5",
    "mercury-sdk": "^0.1.22"
  }
}
